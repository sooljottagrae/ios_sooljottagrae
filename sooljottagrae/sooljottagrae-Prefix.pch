//Change UIColor Hex To RGB
#define UIColorFromRGB(rgbValue, alphaValue) \
[UIColor colorWithRed:((float)((rgbValue & 0xFF0000) >> 16))/255.0 \
green:((float)((rgbValue & 0x00FF00) >>  8))/255.0 \
blue:((float)((rgbValue & 0x0000FF) >>  0))/255.0 \
alpha:alphaValue]


//테마 컬러
//배경
#define THEMA_BG_COLOR UIColorFromRGB(0x71B743,1.0f)
//글
#define THEMA_FT_COLOR UIColorFromRGB(0xFFFFFF,1.0f)
//imageView 테두리
#define THEMA_CIRCLE_COLOR UIColorFromRGB(0xEFEFF4,1.0f)
//어두운 테마 컬러
#define THEMA_BG_DARK_COLOR UIColorFromRGB(0x084A17,1.0f)